{
  "name": "mpg123n",
  "description": "NodeJS native bindings to mpg123 'control_generic' command line interface.",
  "keywords": [
    "mpg123",
    "mp3",
    "mpeg",
    "mpg",
    "encode",
    "decode"
  ],
  "version": "0.0.17",
  "author": "Marek BÄ™dkowski <marek@bedkowski.pl> (http://thejsteam.com)",
    "repository": {
    "type": "git",
    "url": "git://github.com/bendi/node-mpg123n.git"
  },
  "main": "./index.js",
  "dependencies": {
    "bindings": "*",
    "debug": "*"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "binary": {
    "module_name": "bindings",
    "module_path": "./build/Release",
    "remote_uri": "https://mpg123n.s3-us-west-2.amazonaws.com/",
    "template": "{configuration}/{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz"
  }
